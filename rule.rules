rule "get openHAB's Live status from status.openhab.org"
when
    Time cron "0 0 * * * ? *"
then
    executeCommandLine("/usr/bin/python3","/home/openhabvm/openhab_scripts/python/status_openhab.py")
end

rule "get openHAB's Live status from status.openhab.org myopenHABCloudServiceDowntimeLastHour"
when
    Item myopenHABCloudServiceDowntimeLastHour changed
then
     changed_str = myopenHABCloudServiceDowntimeLastHour.replace("hrs", "Std.")
     myopenHABCloudServiceDowntimeLastHour.postUpdate(changed_str)
end

